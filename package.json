{
  "name": "@peculiar/fortify-website",
  "private": true,
  "version": "1.0.0",
  "description": "Fortify enables web applications to use smart cards, local certificate stores and do certificate enrollment.",
  "author": "PeculiarVentures Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/fortify.git"
  },
  "scripts": {
    "prebuild": "npm run build:pages",
    "build": "stencil build",
    "prestart": "npm run build:pages",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "build:pages": "ts-node -P scripts/tsconfig.json scripts/build_pages",
    "clean": "rimraf www src/pages/**/*.json"
  },
  "devDependencies": {
    "@stencil/core": "^1.13.0",
    "@stencil/sass": "^1.3.1",
    "@types/node": "^14.0.5",
    "@types/prismjs": "^1.16.1",
    "fast-glob": "^3.2.2",
    "front-matter": "^4.0.0",
    "fs-extra": "^9.0.0",
    "listr": "^0.14.3",
    "marked": "^1.1.0",
    "prismjs": "^1.20.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1"
  },
  "dependencies": {
    "@stencil/router": "^1.0.1"
  }
}
