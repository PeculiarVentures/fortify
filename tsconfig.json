{
  "compilerOptions": {
    "target": "es2019",   
    "module": "commonjs", 
    "lib": [
      "es2019",
      "dom"
    ],       
    "jsx": "react",     
    "strict": true,    
    "moduleResolution": "node",  
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,           
    "emitDecoratorMetadata": true,          
    "skipLibCheck": true  
  }
}
